<h2>Welcome <%= @user.username %> !</h2>

<a href="../mails"> Messenging </a> <br><br>
<a href="#"> File Transfer </a><br><br>
<a href="/users/logout"> Log out </a><br><br>


<% unless @users.nil? %>
	<hr width="1pt">                                                                                                   
	<table>
	    <thead>
	  <tr>
	      <td> User </td>
	      <td> Password </td>
	      <td> Super </td>
	  </tr>
	    </thead>
	    <tbody>
	  <% @users.each do |user| %>
	  <tr>
	      <td> <%= user.username %> </td>
	      <td> <%= user.password %> </td>
	      <td> <%= (user.super)? "Y" : "" %> </td>
	  </tr>
	  <% end %>
	    </tbody>
	</table>
<% end %>


